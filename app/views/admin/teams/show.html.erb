<%= render 'page_header' %>
<%= render 'application/messages' %>

<%= form_for [:admin, @team] do |f| %>
	<div class="form-group">
    <%= f.label :name %>
	  <p class="form-control-static"><%= @team.name %></p>
	</div>

	<div class="form-group">
    <%= f.label :members %>
	  <%= simple_format @team.members, class: 'form-control-static' %>
	</div>
<% end %>
